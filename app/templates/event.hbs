<div class="p-4">
  <div class="hidden">
    {{#each @model.stats as |stat|}}
      <div class="mb-4">
        <span class="block">{{stat.mark}}</span>
        <span class="block text-xs">{{stat.meetName}}</span>
      </div>
    {{/each}}
  </div>
  <EventProgression @eventData={{@model.stats}} @eventCode = {{@model.code}} @athlete={{@model.athlete}}/>
</div>